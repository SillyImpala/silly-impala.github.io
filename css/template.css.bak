*{
	color:#f3f6da;
	margin:0px;
	font-family:Courier New;
}

::selection{
	color:#e5e5e5;
	background-color:f49ac1;
}

/* width */
::-webkit-scrollbar {
  width: 10px;
}

/* Track */
::-webkit-scrollbar-track {
  background: #101010;
}

/* Handle */
::-webkit-scrollbar-thumb {
  background: #333;
  border-radius:10px;
}

/* Handle on hover */
::-webkit-scrollbar-thumb:hover {
  background: #6dcff6;
}